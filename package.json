{
  "publisher": "sransara",
  "name": "speedscope-in-vscode",
  "displayName": "Speedscope in VSCode",
  "description": "View speedscope flamegraphs in VSCode",
  "repository": {
    "type": "git",
    "url": "https://github.com/sransara/speedscope-in-vscode.git"
  },
  "version": "0.0.2",
  "license": "MIT",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [],
  "extensionKind": [
    "workspace"
  ],
  "icon": "media/icon.png",
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "speedscope-in-vscode.openSpeedscope",
        "title": "Open speedscope"
      }
    ],
    "customEditors": [
      {
        "viewType": "speedscope-in-vscode.speedscope",
        "displayName": "Speedscope",
        "priority": "option",
        "selector": [
          {
            "filenamePattern": "*"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "build-external-deps": "external/speedscope/scripts/build-for-vscode.sh",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "vsce": "^2.15.0",
    "webpack-cli": "^5.1.4",
    "webpack": "^5.88.2"
  }
}
